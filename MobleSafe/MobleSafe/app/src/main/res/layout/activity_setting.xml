<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:yeyy="http://schemas.android.com/apk/res/moblesafe.ncs.yeyy.moblesafe"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="moblesafe.ncs.yeyy.moblesafe.activity.SettingActivity">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#8866ff00"
        android:gravity="center"
        android:text="设置中心"
        android:textColor="@color/colorBlack"
        android:textSize="22sp" />

    <moblesafe.ncs.yeyy.moblesafe.view.SettingItemView
        android:id="@+id/siv_update"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        yeyy:desc_off="自动更新已关闭"
        yeyy:desc_on="自动更新已开启"
        yeyy:title="自动更新设置" />

    <moblesafe.ncs.yeyy.moblesafe.view.SettingItemView
        android:id="@+id/siv_address"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        yeyy:desc_off="归属地显示已经关闭"
        yeyy:desc_on="归属地显示已经开启"
        yeyy:title="电话归属地显示设置" />
    <moblesafe.ncs.yeyy.moblesafe.view.SettingClickView
        android:id="@+id/scv_address_style"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <moblesafe.ncs.yeyy.moblesafe.view.SettingClickView
        android:id="@+id/scv_address_location"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>
    <moblesafe.ncs.yeyy.moblesafe.view.SettingItemView
        android:id="@+id/siv_callsafe"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        yeyy:desc_off="黑名单更新已关闭"
        yeyy:desc_on="黑名单更新已开启"
        yeyy:title="黑名单更新设置" />
</LinearLayout>
